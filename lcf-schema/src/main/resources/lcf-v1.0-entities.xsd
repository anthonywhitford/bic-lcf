<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://ns.bic.org.uk/lcf/1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ns.bic.org.uk/lcf/1.0" elementFormDefault="qualified">
	<xs:element name="authorisation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="https://github.com/anthonywhitford/bic-lcf/wiki/LCF-1.0.1-Information-Entity-XML-bindings#ElementID">E13D01</xs:appinfo>
						</xs:annotation>
				</xs:element>
				<xs:group ref="authorisation-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="authorisation-props">
		<xs:sequence>
			<xs:element ref="authorisation-type" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="https://github.com/anthonywhitford/bic-lcf/wiki/LCF-1.0.1-Information-Entity-XML-bindings#ElementID">E13D02</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="heading" minOccurs="0">
				<xs:annotation>
					<xs:appinfo source="https://github.com/anthonywhitford/bic-lcf/wiki/LCF-1.0.1-Information-Entity-XML-bindings#ElementID">E13D03</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo source="https://github.com/anthonywhitford/bic-lcf/wiki/LCF-1.0.1-Information-Entity-XML-bindings#ElementID">E13C04</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:element name="charge">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="charge-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="charge-props">
		<xs:sequence>
			<xs:element ref="patron-ref"/>
			<xs:element ref="charge-type"/>
			<xs:element ref="charge-status"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="item-ref" minOccurs="0"/>
			<xs:element ref="manifestation-ref" minOccurs="0"/>
			<xs:element ref="loan-ref" minOccurs="0"/>
			<xs:element ref="reservation-ref" minOccurs="0"/>
			<xs:element ref="creation-date" minOccurs="0"/>
			<xs:element ref="payment-due-date" minOccurs="0"/>
			<xs:element ref="charge-amount"/>
			<xs:element ref="currency" minOccurs="0"/>
			<xs:element ref="paid-amount" minOccurs="0"/>
			<xs:element ref="due-amount" minOccurs="0"/>
			<xs:element ref="paid-date" minOccurs="0"/>
			<xs:element ref="payment-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="class-scheme">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="class-scheme-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="class-scheme-props">
		<xs:sequence>
			<xs:element ref="name"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="class-term">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="class-term-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="class-term-props">
		<xs:sequence>
			<xs:element ref="class-code"/>
			<xs:element ref="class-scheme-ref"/>
			<xs:element ref="heading" minOccurs="0"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="contact">
		<xs:annotation>
			<xs:documentation>Represents a single method of communication with a 'patron'. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="contact-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="contact-props">
		<xs:sequence>
			<xs:element ref="patron-ref"/>
			<xs:element ref="communication-type"/>
			<xs:element ref="locator" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="item">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="item-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="item-props">
		<xs:sequence>
			<xs:element ref="additional-item-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="manifestation-ref"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="owner" minOccurs="0"/>
			<xs:element ref="associated-location" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="media-warning"/>
			<xs:element ref="security-desensitize"/>
			<xs:element ref="loan-restriction" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="loan-fee" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="circulation-status"/>
			<xs:element ref="reservation-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="patrons-in-hold-queue" minOccurs="0"/>
			<xs:element ref="on-loan-ref" minOccurs="0"/>
			<xs:element ref="condition-code" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="condition-description" minOccurs="0"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="loan">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="loan-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="loan-props">
		<xs:sequence>
			<xs:element ref="patron-ref"/>
			<xs:element ref="item-ref"/>
			<xs:element ref="start-date"/>
			<xs:element ref="end-due-date" minOccurs="0"/>
			<xs:element ref="end-date" minOccurs="0"/>
			<xs:element ref="loan-status" maxOccurs="unbounded"/>
			<xs:element ref="recall-notice-date" minOccurs="0"/>
			<xs:element ref="charge-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="location">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="location-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="location-props">
		<xs:sequence>
			<xs:element ref="additional-location-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="name" minOccurs="0"/>
			<xs:element ref="location-type" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="manifestation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="manifestation-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="manifestation-props">
		<xs:sequence>
			<xs:element ref="additional-manifestation-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="media-type" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="title" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="contributor" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="series" minOccurs="0"/>
			<xs:element ref="edition-statement" minOccurs="0"/>
			<xs:element ref="publisher-name" minOccurs="0"/>
			<xs:element ref="year-of-publication" minOccurs="0"/>
			<xs:element ref="classification" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="cover-art" minOccurs="0"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="loan-restriction" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="loan-fee" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="patrons-in-hold-queue" minOccurs="0"/>
			<xs:element ref="manifestation-record" minOccurs="0"/>
			<xs:element ref="manifestation-status"/>
			<xs:element ref="items-in-stock" minOccurs="0"/>
			<xs:element ref="item-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="reservation-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="message-alert">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="message-alert-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="message-alert-props">
		<xs:sequence>
			<xs:element ref="authority-ref" minOccurs="0"/>
			<xs:element ref="message-type"/>
			<xs:element ref="priority" minOccurs="0"/>
			<xs:element ref="display-type" minOccurs="0"/>
			<xs:element ref="display-constraint" minOccurs="0"/>
			<xs:element ref="start-date" minOccurs="0"/>
			<xs:element ref="end-date" minOccurs="0"/>
			<xs:element ref="audience" minOccurs="0"/>
			<xs:element ref="patron-category" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="patron-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="message-text" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="patron">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="patron-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="patron-props">
		<xs:sequence>
			<xs:element ref="barcode-id" minOccurs="0"/>
			<xs:element ref="additional-patron-id" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="name"/>
			<xs:element ref="contact-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="language" minOccurs="0"/>
			<xs:element ref="associated-location" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="patron-status" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="card-status-info" minOccurs="0"/>
			<xs:element ref="patron-category" minOccurs="0"/>
			<xs:element ref="patron-tag" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="patron-expiration-date" minOccurs="0"/>
			<xs:element ref="associated-patron-group" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="loan-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="on-loan-items" minOccurs="0"/>
			<xs:element ref="loan-items-limit" minOccurs="0"/>
			<xs:element ref="overdue-items" minOccurs="0"/>
			<xs:element ref="overdue-items-limit" minOccurs="0"/>
			<xs:element ref="recalled-items" minOccurs="0"/>
			<xs:element ref="fees-due-items" minOccurs="0"/>
			<xs:element ref="fines-due-items" minOccurs="0"/>
			<xs:element ref="reservation-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="available-hold-items" minOccurs="0"/>
			<xs:element ref="unavailable-hold-items" minOccurs="0"/>
			<xs:element ref="hold-items-limit" minOccurs="0"/>
			<xs:element ref="charge-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="charge-limit" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="deposit-balance" minOccurs="0"/>
			<xs:element ref="associated-message" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="date-of-birth" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="card-status-info">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="card-status"/>
				<xs:element ref="blocked-card-message" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="payment">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="payment-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="payment-props">
		<xs:sequence>
			<xs:element ref="patron-ref"/>
			<xs:element ref="payment-type"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="charge-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="payment-date" minOccurs="0"/>
			<xs:element ref="amount"/>
			<xs:element ref="currency" minOccurs="0"/>
			<xs:element ref="payment-status" minOccurs="0"/>
			<xs:element ref="transaction-reference" minOccurs="0"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="property">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="property-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="property-props">
		<xs:sequence>
			<xs:element ref="name" minOccurs="0"/>
			<xs:element name="entity-type" type="entityType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="description" minOccurs="0"/>
			<xs:element ref="value-scheme-ref" minOccurs="0"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="reservation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identifier" minOccurs="0"/>
				<xs:group ref="reservation-props"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:group name="reservation-props">
		<xs:sequence>
			<xs:element ref="reservation-type"/>
			<xs:element ref="patron-ref"/>
			<xs:choice>
				<xs:element ref="manifestation-ref"/>
				<xs:element ref="item-ref"/>
			</xs:choice>
			<xs:element ref="start-date" minOccurs="0"/>
			<xs:element ref="pickup-institution-ref" minOccurs="0"/>
			<xs:element ref="pickup-location-ref" minOccurs="0"/>
			<xs:element ref="pickup-date" minOccurs="0"/>
			<xs:element ref="end-date" minOccurs="0"/>
			<xs:element ref="reservation-status"/>
            <xs:element ref="hold-queue-position" minOccurs="0"/>
			<xs:element ref="loan-ref" minOccurs="0"/>
			<xs:element ref="charge-ref" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="note" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:element name="additional-item-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="item-id-type"/>
				<xs:element ref="type-name" minOccurs="0"/>
				<xs:element ref="value"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-location-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="location-id-type"/>
				<xs:element ref="type-name" minOccurs="0"/>
				<xs:element ref="value"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-manifestation-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="manifestation-id-type"/>
				<xs:element ref="type-name" minOccurs="0"/>
				<xs:element ref="value"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="additional-patron-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="patron-id-type"/>
				<xs:element ref="type-name" minOccurs="0"/>
				<xs:element ref="value"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="address-line" type="nonEmptyString"/>
	<xs:element name="amount" type="xs:decimal"/>
	<xs:element name="associated-location">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="association-type" type="locationAssociationType"/>
				<xs:element ref="location-ref"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="associated-message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="message-ref"/>
				<xs:element ref="delivery-status"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="association-type" type="locationAssociationType"/>
	<xs:element name="associated-patron-group">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="association-type" type="patronGroupAssociationType"/>
                <xs:element ref="group-type" minOccurs="0"/>
                <xs:element ref="patron-group-id" minOccurs="0"/>
                <xs:element ref="lead-patron-ref" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="patron-ref" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
	<xs:element name="audience" type="messageAlertAudience"/>
	<xs:element name="authorisation-type" type="authorisationCode"/>
	<xs:element name="authority-ref" type="nonEmptyString"/>
	<xs:element name="available-hold-items" type="xs:int"/>
	<xs:element name="barcode-id" type="nonEmptyString"/>
	<xs:element name="blocked-card-message" type="nonEmptyString"/>
	<xs:element name="card-status" type="cardStatus"/>
	<xs:element name="characteristic-type" type="loanCharacteristicType"/>
	<xs:element name="charge-amount" type="xs:decimal"/>
	<xs:element name="charge-limit">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="charge-type" minOccurs="0"/>
				<xs:element ref="amount"/>
				<xs:element ref="currency" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="charge-ref" type="nonEmptyString"/>
	<xs:element name="charge-status" type="chargeStatus"/>
	<xs:element name="charge-type" type="chargeType"/>
	<xs:element name="circulation-status" type="circulationStatusCode"/>
	<xs:element name="class-code" type="nonEmptyString"/>
	<xs:element name="class-scheme-ref" type="nonEmptyString"/>
	<xs:element name="class-term-ref" type="nonEmptyString"/>
	<xs:element name="classification">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="class-scheme-ref"/>
				<xs:element ref="class-term-ref"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="communication-type" type="communicationType">
		<xs:annotation>
			<xs:documentation>Value from the CMT list, which determines how the locator multiplicity should work. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="condition-code" type="conditionCode"/>
	<xs:element name="condition-description" type="nonEmptyString"/>
	<xs:element name="contact-ref" type="nonEmptyString">
		<xs:annotation>
			<xs:documentation>Link to the methods of communication for the 'patron'. There ought to be at least one method of communication, but this has been reduced to a zero limit due to test data observed in customer deployments. </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="contributor">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="contributor-role"/>
				<xs:choice>
					<xs:element ref="contributor-name"/>
					<xs:element ref="unnamed-contributor"/>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="contributor-name" type="nonEmptyString"/>
	<xs:element name="contributor-role" type="contributorRole"/>
	<xs:element name="cover-art" type="xs:anyURI"/>
	<xs:element name="creation-date" type="xs:dateTime"/>
	<xs:element name="currency" type="iso4217CurrencyCode"/>
	<xs:element name="date-of-birth" type="xs:date"/>
	<xs:element name="deposit-balance" type="xs:decimal"/>
	<xs:element name="description" type="nonEmptyString"/>
	<xs:element name="delivery-status" type="messageAlertDeliveryStatus"/>
	<xs:element name="display-constraint" type="messageAlertDisplayConstraint"/>
	<xs:element name="display-type" type="messageDisplayType"/>
	<xs:element name="due-amount" type="xs:decimal"/>
	<xs:element name="due-date-time" type="xs:dateTime"/>
	<xs:element name="edition-statement" type="nonEmptyString"/>
	<xs:element name="end-date" type="xs:dateTime"/>
	<xs:element name="end-due-date" type="xs:dateTime"/>
	<xs:element name="fee-description" type="nonEmptyString"/>
	<xs:element name="fee-due-date-time" type="xs:dateTime"/>
	<xs:element name="fee-limit">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="charge-type" minOccurs="0"/>
				<xs:element ref="amount"/>
				<xs:element ref="currency" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="fees-due-items" type="xs:int"/>
	<xs:element name="fines-due-items" type="xs:int"/>
    <xs:element name="group-type" type="nonEmptyString"/>
	<xs:element name="heading" type="nonEmptyString"/>
	<xs:element name="hold-items-limit" type="xs:int"/>
	<xs:element name="hold-queue-position" type="xs:int"/>
	<xs:element name="identifier" type="nonEmptyString">
		<xs:annotation>
			<xs:documentation>Unique identifier, allowing the underlying database to control uniqueness of this record. The underlying system will allocate this id. Do not pass this while creating an entity.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="item-id-type" type="copyIDType"/>
	<xs:element name="item-ref" type="nonEmptyString"/>
	<xs:element name="items-in-stock" type="xs:int"/>
	<xs:element name="language" type="languageCode"/>
    <xs:element name="lead-patron-ref" type="nonEmptyString"/>
	<xs:element name="loan-fee">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="charge-type"/>
				<xs:element ref="amount"/>
				<xs:element ref="currency" minOccurs="0"/>
			</xs:sequence>
